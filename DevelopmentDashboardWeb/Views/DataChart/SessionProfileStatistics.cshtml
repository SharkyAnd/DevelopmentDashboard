@{
    ViewBag.Title = "Development Dashboard - Session Profile Statistics";
}
<link href="~/Content/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="~/Content/session/accordion.css" rel="stylesheet" type="text/css" />
<script src="~/Scripts/dx.webappjs.js" type="text/javascript"></script>
<script src="~/Scripts/dx.chartjs.js" type="text/javascript"></script>
<script src="~/Scripts/DataView/accordion.js" type="text/javascript"></script>
<script src="~/Scripts/DataChart/profile-statisticss.js" type="text/javascript"></script>
<div id="chartSettings"  style:"margin-bottom:10px" class="accordion" data-get-parameters = "@Url.Action("SessionProfileStatGetAdditionalDataJson", "DataChart")">
    <div class="accordion-section">
        <a class="accordion-section-title active" href="#accordion-1">Chart Settings: <i id="settings-string"></i><i id="arrow" class="fa fa-angle-down"></i></a>
        <div id="accordion-1" class="accordion-section-content container-fluid">
        <div class="row">
            <div id="period" class="col-md-4">
                <div>
                    From</div>
                <div id="from">
                </div>
                <div>
                    To</div>
                <div id="to">
                </div>
                <div>
                    Aggregation mode</div>
                <div id="aggregation-mode">
                </div><div>
                    Aggregation operation</div>
                <div id="aggregation-operation">
                </div>           
                <div>
                    Aggregation interval</div>
                <div id="aggregation-interval">
                </div>                       
            </div>
            <div class="col-md-4">
                <div>
                    User names</div>
                <div id="user-names">
                </div>
            </div>
            <div class="col-md-4">
                <div>
                    Machine names</div>
                <div id="machine-names">
                </div>
            </div>
            </div>
            <div class="row"><div class="col-md-offset-5 col-md-1" id="create-chart"></div></div>
        </div>
        <!--end .accordion-section-content-->
    </div>
    <!--end .accordion-section-->
</div>
<!--end .accordion-->
<div id="profile-stat-chart"  style="margin-top:20px;" data-get-source="@Url.Action("GetSessionsProfileStatJson", "DataChart")" 
data-get-moment-sessions = "@Url.Action("GetMomentSessionsProfilesJson", "DataChart")"></div>
