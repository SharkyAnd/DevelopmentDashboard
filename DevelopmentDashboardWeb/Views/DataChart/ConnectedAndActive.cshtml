@{
    ViewBag.Title = "DevelopmentDashboard - Connected And Active Sessions";
}
<link href="~/Content/session/ConnectedAndActive.css" rel="stylesheet" type="text/css" />
<script src="~/Scripts/knockout-3.4.0.js" type="text/javascript"></script>
<script src="~/Scripts/dx.webappjs.js" type="text/javascript"></script>
<script src="~/Scripts/dx.chartjs.js" type="text/javascript"></script>
<div class="container">
    <div class="row">
        <div id="period" class="col-md-4">
            <div>
                From</div>
            <div id="from">
            </div>
            <div>
                To</div>
            <div id="to">
            </div>
        </div>
        <div class="col-md-4">
            <div>
                Interval</div>
            <div id="numericBox">
            </div>
            <div>
                Group by</div>
            <div id="selectBox">
            </div>
        </div>
        <div class="col-md-4">
            <div>
                User names</div>
            <div id="user-names">
            </div>
        </div>
    </div>
    <div class="row">
        <div style="margin-top: 10px; margin-bottom: 10px" class="col-md-offset-3 col-md-1"
            id="create-chart">
        </div>
    </div>
</div>
<div id="container" style="margin-top:20px;">
    <div id="zoomedChart" data-get-source="@Url.Action("GetConnectedAndActiveSessionsJson", "DataChart")" data-get-parameters = "@Url.Action("GetConnectedAndActiveParametersJson", "DataChart")"
    data-get-moment-sessions = "@Url.Action("GetMomentSessionsJson", "DataChart")">
    </div>
    <div id="rangeSelector">
    </div>
</div>
<div class="bubblingG">
    <span id="bubblingG_1"></span><span id="bubblingG_2"></span><span id="bubblingG_3">
    </span>
</div>
<script src="~/Scripts/DataChart/connected-and-active.js" type="text/javascript"></script>
